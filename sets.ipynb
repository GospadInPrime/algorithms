     "text": [
      "{'tomato', 'avacado', 'beets', 'carrots', 'banana'}\n",
      "{'tomato'}\n",
      "{'avacado', 'banana'}\n",
      "{'beets', 'carrots'}\n"
     ]
    }
   ],
   "source": [
    "fruits = set(['avacado', 'tomato', 'banana'])\n",
    "vegetables = set(['beets', 'carrots', 'tomato'])\n",
    "# Объединение\n",
    "print(fruits | vegetables)\n",
    "# Пересечение\n",
    "print(fruits & vegetables)\n",
    "# Разность\n",
    "print(fruits - vegetables)\n",
    "print(vegetables - fruits)\n"
   ]
  }
